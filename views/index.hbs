    crossorigin="anonymous" referrerpolicy="no-referrer" />

<script src="https://kit.fontawesome.com/a076d05399.js" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="/static/css/style.css">


  <title>Hello, world!</title>
</head>

<body>
  {{>navbar data=details}}

  {{!-- yaha data transfer kr rhe hai slides vali --}}
  {{> slider slides=slides}}


  {{!-- this is banner section  --}}

<section class="banner1_section py-5 background2">
  <div class="container text-center">
    <div class="row">
      {{!-- div ka ek column jo 12 grid le lega --}}
      <div class="col-md-12">
        <h1>Start learning new technology with SKY</h1>
        <p>Here you can learn lot of thing like Master in JAVA, PYTHON, C++ , C, HTML, CSS, JS, BOOTSTRAP, NODE JS</p>
      
      <button class="btn-outline-primary">Start Learning</button>
      </div>
    </div>
  </div>
</section>




  {{!-- service section --}}

  <section id="services_section" class="py-3">

    <div class="container">

      <h1 class="text-center  text-secondry">Our courses we will provide</h1>

      <div class="row mt-5">
        {{!-- first service column --}}
        {{!-- yaha hum 4 items ko manage kr rhe hai ke column mai --}}
        {{#each course}}
        <div class="col-md-4">
          <div class="card">
            <div class="card-body">
              <div class="container text-center py-3">
                {{!-- yaha neeche text-secondry se color change kr skte hai --}}
                <i class="{{this.icon}}  service_icon"></i>
              </div>
              <h5 class="card-title">{{this.title}}</h5>
              <p>{{this.description}} </p>
              <a href="{{this.linkText}}">{{this.link}}</a>
              <a href="#!" class="ml-2">support</a>

            </div>
          </div>

        </div>
        {{/each}}
      </div>


    </div>
  </section>



  {{!-- contact us section --}}
  <section id="contact_use_section" class="py-5 background1">
    <div class="container">
      <h3 class="text-center text-secondery">Contact us</h3>
      <div class="row">
        {{!-- one column jo ki 12 grade use kr rha hai --}}
        <div class="col-md-12">
          <form action="/process-contact-form" method="post">
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Email ID</label>
              <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
              <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>
            <div class="mb-3">
              <label for="contactNumberField" class="form-label">Mobile Number</label>
              <input name="phone" type="number" class="form-control" id="contactNumberField">
            </div>


            <div class="form-floating mt-2">
              <textarea name="query" style="height:250px " class="form-control" placeholder="Leave a comment here" id="queryField"></textarea>
              <label for="queryField" class="text-dark">Comments</label>
            </div>

            <div class="container text-center">
              <button type="submit" class="btn btn-primary">Submit</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>

{{!-- about section  --}}

